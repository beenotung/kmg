{
  "name": "ionic-horizon-seed",
  "version": "0.0.1",
  "description": "A template Ionic project with Horizon and unit-test",
  "private": true,
  "author": {
    "name": "Beeno Tung",
    "email": "aabbcc1241@yahoo.com.hk",
    "url": "https://www.facebook.com/messages/t/beenotung"
  },
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/beenotung/ionic-horizon-seed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/ionic-horizon-seed.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/ionic-horizon-seed/issues"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "keywords": [
    "angular",
    "angular4",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "ionic",
    "library",
    "ng",
    "typescript",
    "typing",
    "typestub",
    "common"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "build:ionic": "npm run ionic:build",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean": "npm run clean:tmp && rimraf node_modules && npm run ionic:clean",
    "clean:tmp": "npm run dist:unmove && rimraf coverage dist tmp docs www",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage --disablePrivateOrInternalSupport",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "npm run tslint && npm run lint:ionic",
    "tslint": "tslint --type-check --project tsconfig.json -c tslint.json",
    "fix-lint": "tslint --fix -p .",
    "lint:ionic": "ionic-app-scripts lint",
    "ng": "ng",
    "ngcompile": "ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci && npm run dist:move",
    "preversion": "npm run ci",
    "predist:move": "npm run dist:unmove",
    "dist:move": "./dist:move",
    "dist:unmove": "./dist:unmove",
    "test": "concurrently --raw \"npm run test:ng\" \"npm run test:ionic\"",
    "test:ng": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "test:ionic": "npm run ionic:test",
    "serve": "npm run ionic:serve",
    "ionic:clean": "ionic-app-scripts clean --tsconfig tsconfig-ionic.json",
    "ionic:build": "ionic-app-scripts build --tsconfig tsconfig-ionic.json",
    "ionic:lint": "ionic-app-scripts lint --tsconfig tsconfig-ionic.json",
    "ionic:serve": "ionic-app-scripts serve --tsconfig tsconfig-ionic.json",
    "ionic:test": "npm run ionic:build --aot && npm run ionic:lint"
  },
  "dependencies": {
    "@angular/common": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@beenotung/tslib": "^7.0.2",
    "@ionic-native/core": "^4.1.0",
    "@ionic-native/geolocation": "^4.1.0",
    "@ionic-native/splash-screen": "^4.1.0",
    "@ionic-native/status-bar": "^4.1.0",
    "@ionic-native/unique-device-id": "^4.1.0",
    "@ionic/storage": "^2.0.1",
    "@ngx-translate/core": "^7.2.0",
    "@ngx-translate/http-loader": "^0.1.0",
    "angularlib": "^1.0.2",
    "cordova-android": "^6.2.3",
    "cordova-browser": "^4.1.0",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-device": "^1.1.6",
    "cordova-plugin-geolocation": "~2.4.3",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-statusbar": "^2.2.3",
    "cordova-plugin-uniquedeviceid": "^1.3.2",
    "cordova-plugin-whitelist": "^1.3.2",
    "css-animator": "^2.1.1",
    "ionic-angular": "^3.7.0",
    "ionic-plugin-keyboard": "^2.2.1",
    "ioniclib": "^4.0.1",
    "ionicons": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "rxjs": "5.4.3",
    "scsslib": "^1.1.0",
    "sw-toolbox": "^3.6.0",
    "sweetalert2": "^6.9.1",
    "zone.js": "0.8.18"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@angular/cli": "^1.4.3",
