    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/tsc-wrapped": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@ionic/app-scripts": "^2.1.4",
    "@ionic/cli-plugin-proxy": "^1.4.11",
