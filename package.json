    "codecov": "^2.2.0",
    "codelyzer": "^3.2.0",
    "concurrently": "^3.4.0",
    "cordova": "7.0.1",
    "css-loader": "^0.28.1",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "ionic": "^3.12.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.1",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "minimatch": "^3.0.4",
    "node-sass": "^4.5.2",
    "raw-loader": "^0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "^6.0.5",
    "source-map-explorer": "^1.3.3",
    "surge": "^0.19.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.0.4",
    "tslint": "^5.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.4.2",
    "typestub-googlemap": "^0.0.2",
    "typestub-horizon-client": "^0.1.8",
    "webpack": "^3.6.0",
